<h2>New Climb</h2>
<%= form_for @climb do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>
  <%= f.label :location, "Location" %>
  <%= f.text_field :location %>
  <%= f.label :length, "Length" %>
  <%= f.number_field :length %>
  <%= f.label :difficulty, "Difficulty" %>
  <%= f.number_field :difficulty %>
  <%= f.submit %>
<% end %>
